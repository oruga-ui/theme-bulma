# editorconfig.org
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
indent_style = space
indent_size = 4

[*.scss]
indent_style = space
indent_size = 4

[*.vue]
indent_style = space
indent_size = 4

[*.ts]
indent_style = space
indent_size = 4

[*.js]
indent_style = space
indent_size = 4

[*.md]
trim_trailing_whitespace = false