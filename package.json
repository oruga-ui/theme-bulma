{
  "name": "@oruga-ui/theme-bulma",
  "version": "0.2.8",
  "description": "Bulma theme for Oruga",
  "license": "MIT",
  "main": "dist/bulma.js",
  "module": "dist/bulma.mjs",
  "unpkg": "dist/bulma.min.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "src",
    "types/*.d.ts"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oruga-ui/theme-bulma.git"
  },
  "bugs": {
    "url": "https://github.com/oruga-ui/theme-bulma/issues"
  },
  "scripts": {
    "dev": "vite --force",
    "dev:link": "npm link @oruga-ui/oruga-next && vite --force",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "rimraf dist && rollup -c",
    "serve": "vite preview"
  },
  "dependencies": {
    "bulma": "0.9.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "@oruga-ui/oruga-next": "0.5.6",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-typescript": "8.3.0",
    "@vitejs/plugin-vue": "1.2.3",
    "@vue/compiler-sfc": "3.0.5",
    "autoprefixer": "10.3.7",
    "core-js": "3.14.0",
    "postcss": "8.3.10",
    "rimraf": "3.0.2",
    "rollup": "2.58.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-sass": "1.2.9",
    "rollup-plugin-terser": "7.0.2",
    "sass": "1.27.0",
    "sass-loader": "8.0.2",
    "tslib": "2.3.1",
    "typescript": "4.3.2",
    "vite": "2.3.7",
    "vue": "3.0.5",
    "vue-router": "4.0.12",
    "vue-tsc": "0.28.9"
  }
}
